<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite parallel="tests" name="Suite">
	<groups>
		<run>
			<include name="purchasejson"></include>
		</run>
	</groups>
  <test thread-count="5" name="Submit Order Test">
    <classes>
      <class name="RahulshettyAcademy.Tests.webTest"/>
    </classes>
  </test> <!-- Test -->


 <test thread-count="5" name="Error Validations Test">
    <classes>
      <class name="RahulshettyAcademy.Tests.validationCredTest"/>
    </classes>
  </test> <!-- Test -->

<test thread-count="5" name="Reading data from Json">
    <classes>
      <class name="RahulshettyAcademy.Tests.webTestJson"/>
    </classes>
  </test> <!-- Test -->

 
</suite> <!-- Suite -->
